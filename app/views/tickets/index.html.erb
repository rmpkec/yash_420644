<div class="page-header">
  <h1>
    Tickets
    <a href="/tickets/new" class="btn btn-lg btn-success">New Ticket</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Source</th>
        <th>Note</th>
        <th>Score</th>
        <th>Last received at</th>
        <th>Schema less</th>
        <th>Closed</th>
        <th>Actions</th>
      </tr>

      <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.source %></td>
        <td><%= ticket.note_id %></td>
        <td><%= ticket.score %></td>
        <td><%= ticket.last_received_at %></td>
        <td><%= ticket.schema_less %></td>
        <td><%= ticket.closed %></td>
        <td>
          <a href="/tickets/<%= ticket.id %>" class="btn btn-primary">Show</a>
          <a href="/tickets/<%= ticket.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_ticket/<%= ticket.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
